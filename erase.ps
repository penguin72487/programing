# 定義資料夾路徑
$folderPath = "C:\gitproject\zero-judge-practice"

# 獲取所有 .exe 檔案
$files = Get-ChildItem -Path $folderPath -Recurse -Filter *.exe

# 初始化計數器
$counter = 0

# 遍歷並刪除 .exe 檔案
foreach ($file in $files) {
    # 刪除檔案
    Remove-Item -Path $file.FullName -Force
    
    # 增加計數器
    $counter++

    # 輸出已刪除的檔案數量
    Write-Output "$counter"
}
